{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts",
        "tsx"
    ],
    "rootDir": "../src",
    "testRegex": ".spec.tsx?$",
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageReporters":["text-lcov"]
}