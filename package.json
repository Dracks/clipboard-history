{
  "name": "clipboard-history",
  "version": "1.0.0",
  "main": "dist/main/index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:core && npm run build:ui",
    "build:core": "tsc -p config/tsconfig.main.json",
    "build:ui": "webpack --config config/webpack.prod.js",
    "dev": "concurrently --handle-input \"wait-on dist/main/index.js && nodemon\" \"tsc -w -p config/tsconfig.main.json\" \"webpack --config config/webpack.dev.js\"",
    "innosetup": "node innosetup.js",
    "start": "tsc -p config/tsconfig.main.json && electron .",
    "test": "jest --config config/jest.json",
    "test:watch": "jest --config config/jest.json --watch",
    "test:cov": "jest --config config/jest.json --coverage",
    "test:travis-cov": "jest --config config/jest.travis.json",
    "package": "npm run build && npm run package:mac && npm run package:windows && npm run package:linux",
    "package:mac": "electron-packager . my-clipboard-manager --overwrite --platform=darwin --arch=x64 --prune=true --out=build --version-string.FileDescription='my-clipboard-manager' --version-string.ProductName='my-clipboard-manager' --icon=\"icons/icon.icns\" --asar",
    "package:windows": "electron-packager . my-clipboard-manager --overwrite --platform=win32 --arch=ia32 --prune=true --out=build --version-string.CompanyName=dotenorio --version-string.FileDescription='my-clipboard-manager' --version-string.ProductName='my-clipboard-manager' --icon=\"icons/icon.ico\" --asar",
    "package:linux": "electron-packager . my-clipboard-manager --overwrite --platform=linux --arch=x64 --prune=true --out=build --version-string.CompanyName=dotenorio --version-string.FileDescription='my-clipboard-manager' --version-string.ProductName='my-clipboard-manager' --icon=\"icons/icon.ico\" --asar"
  },
  "dependencies": {
    "node-notifier": "^5.4.3",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@elastic/datemath": "^5.0.2",
    "@elastic/eui": "^13.7.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/node-notifier": "^5.4.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "concurrently": "^4.1.2",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "electron": "^6.0.3",
    "electron-packager": "^14.0.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-create-mock-instance": "^1.1.0",
    "moment": "^2.24.0",
    "nodemon": "^1.19.1",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "source-map-loader": "^0.2.4",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "wait-on": "^3.3.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-merge": "^4.2.2"
  }
}
